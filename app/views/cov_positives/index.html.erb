<p id="notice"><%= notice %></p>

<h1>Cov Positives</h1>

<table>
  <thead>
    <tr>
      <th>City</th>
      <th>Amount</th>
      <th>Datetime</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cov_positives.each do |cov_positive| %>
      <tr>
        <td><%= cov_positive.city.name %></td>
        <td><%= cov_positive.amount %></td>
        <td><%= cov_positive.dateTime %></td>
        <td><%= link_to 'Show', cov_positive %></td>
        <td><%= link_to 'Edit', edit_cov_positive_path(cov_positive) %></td>
        <td><%= link_to 'Destroy', cov_positive, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= paginate @cov_positives %>

<%= link_to 'New Cov Positive', new_cov_positive_path %>


<%= link_to "Log out", destroy_user_session_path, method: "delete" %>
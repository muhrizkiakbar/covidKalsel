<p id="notice"><%= notice %></p>

<h1>Cov Dieds</h1>

<table>
  <thead>
    <tr>
      <th>City</th>
      <th>Amount</th>
      <th>Datetime</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cov_dieds.each do |cov_died| %>
      <tr>
        <td><%= cov_died.city %></td>
        <td><%= cov_died.amount %></td>
        <td><%= cov_died.dateTime %></td>
        <td><%= link_to 'Show', cov_died %></td>
        <td><%= link_to 'Edit', edit_cov_died_path(cov_died) %></td>
        <td><%= link_to 'Destroy', cov_died, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cov Died', new_cov_died_path %>

<p id="notice"><%= notice %></p>

<h1>Cov Negatives</h1>

<table>
  <thead>
    <tr>
      <th>City</th>
      <th>Amount</th>
      <th>Datetime</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cov_negatives.each do |cov_negative| %>
      <tr>
        <td><%= cov_negative.city %></td>
        <td><%= cov_negative.amount %></td>
        <td><%= cov_negative.dateTime %></td>
        <td><%= link_to 'Show', cov_negative %></td>
        <td><%= link_to 'Edit', edit_cov_negative_path(cov_negative) %></td>
        <td><%= link_to 'Destroy', cov_negative, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= paginate @cov_negatives %>

<%= link_to 'New Cov Negative', new_cov_negative_path %>

<%= link_to "Log out", destroy_user_session_path, method: "delete" %>